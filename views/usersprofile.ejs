<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>CMS Login</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <link rel="stylesheet" href="css/style.css">
    </head>
<body>
    
    <%-include('../partials/header.ejs')%>

    <section id="profiles">
        <div class="container">
            <div class="row">
                <%record.forEach((result)=>{%>
                    <div class="col-md-3">
                        <div class="card" style="width: 18rem;">
                            <img src="/profileimages/<%=result.img%>" class="card-img-top mx-auto d-block" alt="cannot be displayed">
                            <div class="card-body">
                              <h5 class="card-title text-center"><%=result.firstname%> <%=result.lastname%></h5>
                              <p class="card-text "><%=result.desc%></p>
                              <a href="/usersingledetail/<%=result.id%>" class="btn btn-primary">Contact Details...</a>
                            </div>
                          </div>
                    </div>
                <%})%>
                
                
            </div>
        </div>
    </section>

    
    <%-include('../partials/footer.ejs')%>



</body>
</html>